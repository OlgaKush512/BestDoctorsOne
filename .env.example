# Backend Environment Variables
NODE_ENV=development
PORT=5000
FRONTEND_URL=http://localhost:3000

# LLM Provider Configuration (openai or blackbox)
LLM_PROVIDER=openai

# OpenAI API Configuration (if using openai provider)
OPENAI_API_KEY=your_openai_api_key_here

# Blackbox AI API Key (if using blackbox provider via OpenRouter)
# BLACKBOX_API_KEY=your_blackbox_api_key_here

# Optional: If using Google Places API instead of scraping
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Optional: If using official Doctolib API
DOCTOLIB_API_KEY=your_doctolib_api_key_here
DOCTOLIB_API_SECRET=your_doctolib_api_secret_here

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging Configuration
LOG_LEVEL=info
